{
  "name": "tts-pipelines",
  "version": "0.2.0",
  "description": "TTS pipelines for text to speech using Kitten TTS and Piper TTS",
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/esm/index.mjs",
      "types": "./lib/esm/types/index.d.ts"
    }
  },
  "types": "./lib/esm/types/index.d.ts",
  "main": "./lib/esm/index.mjs",
  "files": [
    "lib/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "del-cli ./lib",
    "build": "npm run clean && npm run build:esm",
    "build:esm": "tsc -p ./config/tsconfig.esm.json && move-file lib/esm/index.js lib/esm/index.mjs",
    "prepack": "npm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.{ts,js}"
  },
  "keywords": [
    "onnx",
    "tts",
    "text-to-speech",
    "machine-learning"
  ],
  "homepage": "https://github.com/rahulsushilsharma/tts-pipelines#readme",
  "bugs": {
    "url": "https://github.com/rahulsushilsharma/tts-pipelines/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rahulsushilsharma/tts-pipelines.git"
  },
  "license": "ISC",
  "author": "Rahul Sharma <rahu8299@gmail.com>",
  "peerDependencies": {
    "onnxruntime-web": "^1.23.0",
    "onnxruntime-node": "^1.23.0"
  },
  "peerDependenciesMeta": {
    "onnxruntime-web": {
      "optional": true
    },
    "onnxruntime-node": {
      "optional": true
    }
  },
  "dependencies": {
    "phonemizer": "^1.2.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "onnxruntime-node"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.18",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@webgpu/types": "^0.1.65",
    "del-cli": "^5.1.0",
    "eslint": "^9.27.0",
    "jest": "^29.7.0",
    "jsdoc-to-markdown": "^9.1.1",
    "move-file-cli": "^3.0.0",
    "semantic-release": "^24.2.4",
    "ts-jest": "^29.3.4",
    "typescript": "^5.8.3"
  }
}
